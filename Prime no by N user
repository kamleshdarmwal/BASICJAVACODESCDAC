  
import java.util.Scanner;
 
class Primes {
    public static void main(String args[]) {
        int n,x=2,i;
        Scanner kb=new Scanner(System.in);
        System.out.println("Enter A Value : ");
        n=kb.nextInt();
        
    while(n!=0)
    {
        for(i=2;i<x;i++)
            if(x%i==0)
                break;
            if(i==x)
            {
                System.out.println(x);
                n--;
            }
            x++;
    }
}
}
